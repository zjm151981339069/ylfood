package com.spfood.oms.order.utils;

public enum OrderBillType {

	not(0,"无发票"),
	ORDINARY(1,"普通发票"),
	APPRECIATION(2,"增值税发票"),
	OTHER(3,"其他发票");
	
	private int value;
	private String name;
	
	private OrderBillType(int value,String name){
		this.value = value;
		this.name = name;
	}
	public int getValue() {
		return value;
	}
	public String getName() {
		return name;
	}
}
